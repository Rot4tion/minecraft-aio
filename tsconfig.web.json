{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/helpers/**/*",
    "src/shared/**/*",
    "src/types/**/*","src/types.d.ts"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@types/*": [
        "src/types/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/pages/*": [
        "src/renderer/src/pages/*"
      ],
      "@/components/*": [
        "src/renderer/src/components/*"
      ],
      "@/lib/*": [
        "src/renderer/src/lib/*"
      ],
      "@/layouts/*": [
        "src/renderer/src/layouts/*"
      ],
      "@/helpers/*": [
        "src/helpers/*"
      ],
    }
  }
}
